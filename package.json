{
  "name": "navi-mcp-codebase-navigator",
  "version": "1.0.0",
  "description": "Advanced MCP Codebase Navigator with ASCII tree visualization, dependency analysis, semantic search, and authentication discovery",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "clean": "rm -rf dist/",
    "test:mcp": "echo '{\"jsonrpc\":\"2.0\",\"id\":1,\"method\":\"tools/list\"}' | npm start",
    "demo:tree": "echo '{\"jsonrpc\":\"2.0\",\"id\":1,\"method\":\"tools/call\",\"params\":{\"name\":\"generate-tree\",\"arguments\":{\"path\":\".\"}}}' | npm start",
    "demo:search": "echo '{\"jsonrpc\":\"2.0\",\"id\":1,\"method\":\"tools/call\",\"params\":{\"name\":\"semantic-search\",\"arguments\":{\"query\":\"authentication\",\"path\":\".\"}}}' | npm start"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "codebase-navigator",
    "dependency-analysis",
    "semantic-search",
    "authentication-discovery",
    "ascii-tree",
    "code-analysis",
    "ai-tools"
  ],
  "author": "NAVI Development Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "zod": "^3.23.8",
    "@babel/parser": "^7.23.0",
    "@typescript-eslint/parser": "^8.8.0",
    "tree-sitter": "^0.21.0",
    "tree-sitter-javascript": "^0.21.0",
    "tree-sitter-typescript": "^0.21.0",
    "tree-sitter-python": "^0.21.0",
    "tree-sitter-go": "^0.21.0",
    "tree-sitter-java": "^0.21.0",
    "graphlib": "^2.1.8",
    "chalk": "^5.3.0",
    "glob": "^10.3.0",
    "fuse.js": "^7.0.0",
    "natural": "^6.12.0",
    "minimatch": "^9.0.0",
    "uuid": "^10.0.0",
    "fast-glob": "^3.3.0",
    "ignore": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "@types/jest": "^29.5.12",
    "@types/natural": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^9.11.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/navi-mcp-codebase-navigator.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/navi-mcp-codebase-navigator/issues"
  },
  "homepage": "https://github.com/your-org/navi-mcp-codebase-navigator#readme"
}
